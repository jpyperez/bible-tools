describe "Miscellaneous tests", ->
	p = {}
	beforeEach ->
		p = new bcv_parser
		p.set_options book_alone_strategy: "ignore", book_sequence_strategy: "ignore", osis_compaction_strategy: "bc", captive_end_digits_strategy: "delete"
		p.include_apocrypha true

	it "should return the expected language", ->
		expect(p.languages).toEqual ["bbc"]

	it "should handle ranges (bbc)", ->
		expect(p.parse("Titus 1:1 tu 2").osis()).toEqual "Titus.1.1-Titus.1.2"
		expect(p.parse("Matt 1tu2").osis()).toEqual "Matt.1-Matt.2"
		expect(p.parse("Phlm 2 TU 3").osis()).toEqual "Phlm.1.2-Phlm.1.3"
	it "should handle chapters (bbc)", ->
		expect(p.parse("Titus 1:1, pasal 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 PASAL 6").osis()).toEqual "Matt.3.4,Matt.6"
	it "should handle verses (bbc)", ->
		expect(p.parse("Exod 1:1 Ayat 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm AYAT 6").osis()).toEqual "Phlm.1.6"
	it "should handle 'and' (bbc)", ->
		expect(p.parse("Exod 1:1 dohot 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm 2 DOHOT 6").osis()).toEqual "Phlm.1.2,Phlm.1.6"
	it "should handle titles (bbc)", ->
		expect(p.parse("Ps 3 judul, 4:2, 5:judul").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
		expect(p.parse("PS 3 JUDUL, 4:2, 5:JUDUL").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
	it "should handle 'ff' (bbc)", ->
		expect(p.parse("Rev 3ff, 4:2ff").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
		expect(p.parse("REV 3 FF, 4:2 FF").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
	it "should handle translations (bbc)", ->
		expect(p.parse("Lev 1 (bbc)").osis_and_translations()).toEqual [["Lev.1", "bbc"]]
		expect(p.parse("lev 1 bbc").osis_and_translations()).toEqual [["Lev.1", "bbc"]]
	it "should handle boundaries (bbc)", ->
		p.set_options {book_alone_strategy: "full"}
		expect(p.parse("\u2014Matt\u2014").osis()).toEqual "Matt.1-Matt.28"
		expect(p.parse("\u201cMatt 1:1\u201d").osis()).toEqual "Matt.1.1"

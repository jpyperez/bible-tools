describe "Miscellaneous tests", ->
	p = {}
	beforeEach ->
		p = new bcv_parser
		p.set_options book_alone_strategy: "ignore", book_sequence_strategy: "ignore", osis_compaction_strategy: "bc", captive_end_digits_strategy: "delete"
		p.include_apocrypha true

	it "should return the expected language", ->
		expect(p.languages).toEqual ["sn"]

	it "should handle ranges (sn)", ->
		expect(p.parse("Titus 1:1 kushika 2").osis()).toEqual "Titus.1.1-Titus.1.2"
		expect(p.parse("Matt 1kushika2").osis()).toEqual "Matt.1-Matt.2"
		expect(p.parse("Phlm 2 KUSHIKA 3").osis()).toEqual "Phlm.1.2-Phlm.1.3"
	it "should handle chapters (sn)", ->
		expect(p.parse("Titus 1:1, chitsauko 2").osis()).toEqual "Titus.1.1,Titus.2"
		expect(p.parse("Matt 3:4 CHITSAUKO 6").osis()).toEqual "Matt.3.4,Matt.6"
	it "should handle verses (sn)", ->
		expect(p.parse("Exod 1:1 vesi 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm VESI 6").osis()).toEqual "Phlm.1.6"
	it "should handle 'and' (sn)", ->
		expect(p.parse("Exod 1:1 na 3").osis()).toEqual "Exod.1.1,Exod.1.3"
		expect(p.parse("Phlm 2 NA 6").osis()).toEqual "Phlm.1.2,Phlm.1.6"
	it "should handle titles (sn)", ->
		expect(p.parse("Ps 3 title, 4:2, 5:title").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
		expect(p.parse("PS 3 TITLE, 4:2, 5:TITLE").osis()).toEqual "Ps.3.1,Ps.4.2,Ps.5.1"
	it "should handle 'ff' (sn)", ->
		expect(p.parse("Rev 3ff, 4:2ff").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
		expect(p.parse("REV 3 FF, 4:2 FF").osis()).toEqual "Rev.3-Rev.22,Rev.4.2-Rev.4.11"
	it "should handle translations (sn)", ->
		expect(p.parse("Lev 1 (bdsc)").osis_and_translations()).toEqual [["Lev.1", "bdsc"]]
		expect(p.parse("lev 1 bdsc").osis_and_translations()).toEqual [["Lev.1", "bdsc"]]
	it "should handle boundaries (sn)", ->
		p.set_options {book_alone_strategy: "full"}
		expect(p.parse("\u2014Matt\u2014").osis()).toEqual "Matt.1-Matt.28"
		expect(p.parse("\u201cMatt 1:1\u201d").osis()).toEqual "Matt.1.1"
